<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/_home_artificial"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/bj"
                android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/x96"
            android:background="@drawable/intelligenttest_title_backg">


            <ImageView
                android:id="@+id/home_artificial_return"
                android:layout_width="@dimen/x40"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/x26"
                android:layout_marginLeft="@dimen/x30"
                android:layout_marginTop="@dimen/x26"
                android:src="@drawable/intelligenttest_return"/>


            <ImageView
                android:id="@+id/home_artificial_call"
                android:layout_width="@dimen/x40"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x28"
                android:src="@drawable/home_artificial_call"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="客服"
                android:textStyle="bold"
                android:textColor="@color/homeText"
                android:textSize="@dimen/x35"/>


        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/home_artificial_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#A1010000"
        android:orientation="vertical"
        android:visibility="gone">
    </LinearLayout>

</RelativeLayout>
